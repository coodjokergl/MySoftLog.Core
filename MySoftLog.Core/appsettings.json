{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JSON Schema for my JSON file format",

  "type": "object",

  "properties": {
    "name": {
      "type": "string",
      "description": "This shows up in tooltips for the 'name' property."
    },
    "versions": {
      "type": "array",
      "description": "An array of version numbers.",
      "minItems": 1,
      "items": {
        "type": "number"
      }
    }
  },

  "Swagger": {
    "Files": "MySoftLog.Model.xml;MySoftLog.Core.xml;MySoftLog.IService.xml;MySoftLog.IRepository.xml;MysoftLog.DaoShu.xml",
    "Controller": "MySoftLog.Core.xml"
  },
  "LimitRequests": {
    "Url": "http://127.0.0.1:10233"
  }
}
